        <servlet-name>MaestroServ</servlet-name>
        <servlet-class>Control.MaestroServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MarcacionServ</servlet-name>
        <servlet-class>Control.MarcacionServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FitoProductoServ</servlet-name>
        <servlet-class>Control.FitoProductoServ</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UsuarioServ</servlet-name>
        <url-pattern>/usuarios.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PermisoServ</servlet-name>
        <url-pattern>/permisos.do</url-pattern>
    </servlet-mapping>
