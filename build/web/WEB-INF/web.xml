        <servlet-name>ControlCambiosServ</servlet-name>
        <url-pattern>/controlcambios.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FitosanidadServ</servlet-name>
        <url-pattern>/fitosanidad.do</url-pattern>
