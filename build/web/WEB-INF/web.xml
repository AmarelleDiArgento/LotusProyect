        <servlet-class>Control.RolServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AsignaPerServ</servlet-name>
        <servlet-class>Control.AsignaPerServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ArmadoServ</servlet-name>
        <servlet-class>Control.ArmadoServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControlCambiosServ</servlet-name>
        <servlet-class>Control.ControlCambiosServ</servlet-class>
